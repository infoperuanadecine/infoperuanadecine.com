# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# MongoDB Atlas Connection String
# Get this from MongoDB Atlas -> Connect -> Drivers
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/moviedb?retryWrites=true&w=majority

# TMDb API Key for Movie Posters
# Get this from https://www.themoviedb.org/settings/api
TMDB_API_KEY=your_tmdb_api_key_here

# Environment
NODE_ENV=development